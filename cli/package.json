    "commander": "^11.0.0",
    "ws": "^8.18.0",
    "jsonwebtoken": "^9.0.2",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "inquirer": "^9.2.0",
    "js-yaml": "^4.1.0",
    "dotenv": "^16.3.1",
