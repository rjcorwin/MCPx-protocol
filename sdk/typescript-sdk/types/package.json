{
  "name": "@meup/types",
  "version": "0.2.0",
  "description": "TypeScript type definitions for MEUP (Multi-Entity Unified-context Protocol)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "meup",
    "types",
    "typescript",
    "protocol",
    "multi-entity",
    "unified-context"
  ],
  "author": "MEUP Working Group",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mcpx-protocol.git",
    "directory": "sdk/typescript-sdk/types"
  },
  "homepage": "https://github.com/rjcorwin/mcpx-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mcpx-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}